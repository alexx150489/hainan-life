extends ../layout/layout

block title 
	| Список партнеров

block content

	section(class="block-content")
		div(class="site-wrap")
			section(class="block-breadcrumbs")
				a(class="breadcrumbs-item home" href="index.html")
				a(class="breadcrumbs-item" href="#") Партнеры
				span(class="breadcrumbs-item current") Список партнеров


